package com.WebdriverAssignments;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;

public class A08LocateByAbsulateXpath {

	public static void main(String[] args) {
		
driver.manage().window().maximize();
		
		driver.get("https://www.echotrak.com/Login.aspx?ReturnUrl=%2f");
		
		WebElement username = driver.findElement(By.xpath("html/body/div//div/div/div/div/div/div[2]/div/div/form/div/div/input"));
		
	
		username.sendKeys("pravingarade@yahoo.com");
		
		WebElement pass = driver.findElement(By.xpath("html/body/div/div/div/div/div/div/div[2]/div/div/form/div/div[2]/div/input"));
		
		pass.sendKeys("pravin123");
		
		WebElement loginbtn = driver.findElement(By.xpath("html/body/div/div/div/div/div/div/div[2]/div/div/form/div[2]/button"));
		
		loginbtn.click();  
		
	

	}

}
